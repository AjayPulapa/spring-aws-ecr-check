{
  "family": "springboot-task-definition",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "springboot-container",
      "image": "469970949948.dkr.ecr.us-east-1.amazonaws.com/springboot-app:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "memory": 512,
      "cpu": 256
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::469970949948:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::469970949948:role/ecsTaskRole"
}
